import java.io.*;
import java.applet.*;
import java.awt.*;
/*<Applet code="Sample.java"width="500",height="600"></Applet>*/

public class Sample extends Applet
{
	public void paint(Graphics g)
	{
		g.drawOval(20,30,140,300);
		g.drawLine(50,70,35,25);
		g.drawRect(110,140,50,20);
g.fillOval(100,20,50,100);
	}
}
